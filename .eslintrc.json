{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": ["@react-native-community"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "react-native"
      ],
    "rules": {
        "quotes": "off",
        "react/react-in-jsx-scope": "off"
    }
}
